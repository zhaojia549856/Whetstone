#!/bin/sh

count=1



while [ $count != 2 ]; do
	version=1
	while [ $version != 4 ]; do 
		echo "python dense$version.py | tail -n 10 > output"
		python dense$version.py | tail -n 10 > output

		echo "mkdir dense$version_$count" 
		mkdir dense$version_$count

		echo "mv supply.txt whetstone.net danna2.net output dense$version_$count" 
		mv supply.txt whetstone.net danna2.net output dense$version_$count
		
		version=$(( $version + 1 ))

	done
	count=$(( $count + 1 ))
done 



